  "description": "Webhookを介してデプロイスクリプトを実行するためのAPI",
  "author": "nana4rider",
  "license": "ISC",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx --env-file=.env src/index.ts",
    "start": "node dist/index",
    "build": "node build.js",
    "lint": "tsc && eslint --max-warnings 0 && prettier . --check",
    "lint:fix": "eslint --fix && prettier . --write",
